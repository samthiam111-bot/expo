{
    "$schema": "../../../tools/src/prebuilds/schemas/spm.config.schema.json",
    "products": [
        {
            "name": "KeyboardController",
            "podName": "react-native-keyboard-controller",
            "codegenName": "keyboardcontroller",
            "platforms": ["iOS(.v15)"],
            "externalDependencies": [
                "ReactNativeDependencies",
                "React",
                "Hermes"
            ],
            "excludeFromUmbrella": [
                "KeyboardController-Bridging-Header.h"
            ],
            "targets": [
                {
                    "type": "cpp",
                    "name": "KeyboardController_codegen_components",
                    "moduleName": "keyboardcontroller",
                    "path": ".build/codegen/build/generated/ios/ReactCodegen/react/renderer/components/keyboardcontroller",
                    "pattern": "**/*.cpp",
                    "headerPattern": "**/*.h",
                    "dependencies": [
                        "React",
                        "ReactNativeDependencies"
                    ],
                    "includeDirectories": ["../../../.."]
                },
                {
                    "type": "objc",
                    "name": "KeyboardController_codegen_modules",
                    "moduleName": "keyboardcontroller",
                    "path": ".build/codegen/build/generated/ios/ReactCodegen/keyboardcontroller",
                    "pattern": "**/*.mm",
                    "headerPattern": "**/*.h",
                    "dependencies": [
                        "React",
                        "ReactNativeDependencies"
                    ],
                    "includeDirectories": [".."]
                },
                {
                    "type": "cpp",
                    "name": "KeyboardController_common_cpp",
                    "moduleName": "keyboardcontroller",
                    "path": "common/cpp/react/renderer/components/keyboardcontroller",
                    "pattern": "**/*.cpp",
                    "headerPattern": "**/*.h",
                    "dependencies": [
                        "React",
                        "ReactNativeDependencies",
                        "KeyboardController_codegen_components"
                    ],
                    "includeDirectories": [
                        ".",
                        "../../../../../../.build/codegen/build/generated/ios/ReactCodegen"
                    ]
                },
                {
                    "type": "objc",
                    "name": "KeyboardController_objc",
                    "moduleName": "keyboardcontroller",
                    "path": "ios",
                    "pattern": "**/*.m",
                    "headerPattern": "**/*.h",
                    "dependencies": [
                        "React",
                        "ReactNativeDependencies"
                    ],
                    "linkedFrameworks": ["Foundation", "UIKit"],
                    "compilerFlags": [
                        "-include", "Foundation/Foundation.h",
                        "-include", "UIKit/UIKit.h"
                    ]
                },
                {
                    "type": "swift",
                    "name": "KeyboardController_swift",
                    "path": "ios",
                    "pattern": "**/*.swift",
                    "dependencies": [
                        "React",
                        "ReactNativeDependencies",
                        "KeyboardController_objc"
                    ],
                    "linkedFrameworks": ["UIKit", "Foundation", "QuartzCore"]
                },
                {
                    "type": "objc",
                    "name": "KeyboardController",
                    "path": "ios",
                    "pattern": "**/*.mm",
                    "headerPattern": "**/*.h",
                    "exclude": ["views/KeyboardControllerViewManager.mm"],
                    "dependencies": [
                        "Hermes",
                        "React",
                        "ReactNativeDependencies",
                        "KeyboardController_codegen_components",
                        "KeyboardController_codegen_modules",
                        "KeyboardController_common_cpp",
                        "KeyboardController_objc",
                        "KeyboardController_swift"
                    ],
                    "includeDirectories": [
                        ".",
                        "../.build/codegen/build/generated/ios/ReactCodegen",
                        "../common/cpp"
                    ],
                    "linkedFrameworks": ["Foundation", "UIKit", "QuartzCore"],
                    "compilerFlags": [
                        "-include", "UIKit/UIKit.h",
                        "-fcxx-modules"
                    ]
                }
            ]
        }
    ]
}
