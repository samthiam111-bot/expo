{
  "$schema": "../../tools/src/prebuilds/schemas/spm.config.schema.json",
  "products": [
    {
      "name": "ExpoWidgets",
      "podName": "ExpoWidgets",
      "platforms": [
        "iOS(.v15)"
      ],
      "externalDependencies": [
        "ReactNativeDependencies",
        "React",
        "Hermes",
        "expo-modules-core/ExpoModulesCore",
        "@expo/ui/ExpoUI"
      ],
      "targets": [
        {
          "type": "swift",
          "name": "ExpoWidgets",
          "path": "ios",
          "pattern": "**/*.swift",
          "dependencies": [
            "ReactNativeDependencies",
            "React",
            "Hermes",
            "expo-modules-core/ExpoModulesCore",
            "@expo/ui/ExpoUI"
          ],
          "linkedFrameworks": [
            "Foundation",
            "UIKit"
          ]
        }
      ]
    }
  ]
}
